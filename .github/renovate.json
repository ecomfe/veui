{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "repositories": ["ecomfe/veui"],
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "enabled": false
    },
    {
      "matchPackageNames": ["dls-icons-vue"],
      "matchBaseBranches": ["d20"],
      "enabled": true
    }
  ],
  "rangeStrategy": "bump",
  "commitBody": "[skip ci]",
  "constraints": {
    "pnpm": "7.3.0"
  }
}
