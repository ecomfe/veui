{
  "version": "2.6.5",
  "scripts": {
    "dev": "cd packages/veui && pnpm run serve",
    "test": "pnpm -r test",
    "build-components": "cd packages/veui && pnpm run components && pnpm run demo && cd ../..",
    "build-plugin": "cd packages/babel-plugin-veui && pnpm run build && cd ../..",
    "build-loader": "cd packages/veui-loader && pnpm run build && cd ../..",
    "build": "pnpm -r build",
    "prepare": "husky install",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "babel-eslint": "^10.1.0",
    "bumpp": "^7.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^7.0.4",
    "lint-staged": "^10.5.4",
    "micromatch": "^4.0.5",
    "postcss-html": "^1.4.1",
    "postcss-less": "^5.0.0",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^23.0.0",
    "typescript": "^4.6.4"
  }
}
