{
  "name": "veui",
  "version": "2.3.4",
  "description": "Baidu Enterprise UI for Vue.js.",
  "scripts": {
    "serve": "cross-env NODE_OPTIONS=--max_old_space_size=4096 vue-cli-service serve",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 vue-cli-service build",
    "lint": "vue-cli-service lint",
    "components": "node ./build/components.js",
    "index": "node ./build/components.js index",
    "prepublishOnly": "rollup -c && cp -r ./src/* . && cp ../../CHANGELOG.md .",
    "publish": "rm -rf ./common ./components ./directives ./locale ./managers ./mixins ./plugins ./utils ./icons.js ./index.js ./index-with-locale.js ./CHANGELOG.md",
    "test": "cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=test BABEL_ENV=test karma start --single-run",
    "test:start": "cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=test BABEL_ENV=test karma start"
  },
  "module": "index.js",
  "sideEffects": [
    "./src/index-with-locale.js",
    "./index-with-locale.js",
    "./src/locale/**/*.js",
    "./locale/**/*.js",
    "./src/common/uiTypes.js",
    "./common/uiTypes.js",
    "./dist/locale.*.esm.js"
  ],
  "dependencies": {
    "bytes": "^3.0.0",
    "clone-element": "^0.1.0",
    "core-js": "^3.8.3",
    "date-fns": "^2.14.0",
    "dls-graphics": "^1.0.0",
    "lodash": "^4.17.19",
    "popper.js": "^1.16.0",
    "resize-detector": "^0.1.6",
    "vue-awesome": "^4.1.0",
    "vue-directive-normalizer": "^0.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-veui": "^2.3.4",
    "chai": "^4.2.0",
    "classlist-polyfill": "^1.2.0",
    "cross-env": "^5.2.0",
    "esbuild": "^0.12.16",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^6.2.2",
    "focus-visible": "^5.2.0",
    "formidable": "^1.2.2",
    "fs-extra": "^9.1.0",
    "karma": "^5.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-express-server": "^0.1.4",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "less": "^3.13.1",
    "less-loader": "^7.3.0",
    "less-plugin-est": "^3.0.0",
    "lint-staged": "^8.1.4",
    "mocha": "^7.2.0",
    "moment": "^2.27.0",
    "promise-polyfill": "^8.1.0",
    "recursive-readdir": "^2.2.2",
    "rollup": "^2.49.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-vue": "^5.1.9",
    "sinon": "^8.0.4",
    "veui-loader": "^2.3.4",
    "veui-theme-dls": "^2.3.4",
    "veui-theme-dls-icons": "^2.3.4",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.5.16"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "keywords": [
    "vue",
    "ui",
    "components",
    "veui",
    "baidu"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "repository": "ecomfe/veui"
}
