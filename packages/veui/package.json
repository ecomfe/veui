{
  "name": "veui",
  "version": "2.16.2",
  "description": "Baidu Enterprise UI for Vue.js.",
  "scripts": {
    "serve": "cross-env NODE_OPTIONS=\"--max_old_space_size=4096 --openssl-legacy-provider\" vue-cli-service serve",
    "demo": "cross-env NODE_OPTIONS=\"--max_old_space_size=4096 --openssl-legacy-provider\" vue-cli-service build",
    "lint": "vue-cli-service lint",
    "components": "node ./build/components.js",
    "build": "rollup -c",
    "prepublishOnly": "pnpm build && cp -r ./src/* . && cp ../../CHANGELOG.md .",
    "publish": "rm -rf ./common ./components ./directives ./locale ./managers ./mixins ./plugins ./utils ./icons.js ./index.js ./index-with-locale.js ./CHANGELOG.md",
    "test": "cross-env NODE_OPTIONS=\"--max_old_space_size=4096 --openssl-legacy-provider\" NODE_ENV=test BABEL_ENV=test karma start --single-run",
    "test:start": "cross-env NODE_OPTIONS=\"--max_old_space_size=4096 --openssl-legacy-provider\" NODE_ENV=test BABEL_ENV=test karma start"
  },
  "module": "index.js",
  "sideEffects": [
    "./src/index-with-locale.js",
    "./index-with-locale.js",
    "./src/locale/**/*.js",
    "./locale/**/*.js",
    "./src/common/global.js",
    "./common/global.js",
    "./dist/locale.*.esm.js"
  ],
  "types": "./types/index.d.ts",
  "dependencies": {
    "@justfork/maska": "^2.0.0",
    "bytes": "^3.1.2",
    "clone-element": "^0.1.0",
    "core-js": "^3.22.4",
    "date-fns": "^2.28.0",
    "dls-graphics": "^1.4.3",
    "lodash": "^4.17.21",
    "popper.js": "^1.16.1",
    "resize-detector": "^0.1.10",
    "vue-awesome": "^4.5.0",
    "vue-directive-normalizer": "0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/runtime": "^7.17.9",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^19.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "@vue/cli-plugin-babel": "^4.5.17",
    "@vue/cli-plugin-eslint": "^4.5.17",
    "@vue/cli-service": "^4.5.17",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-veui": "workspace:*",
    "chai": "^4.3.6",
    "classlist-polyfill": "^1.2.0",
    "cross-env": "^5.2.1",
    "dls-icons-vue": "~3.28.0",
    "dls-illustrations-vue": "^1.4.3",
    "esbuild": "^0.12.29",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^6.2.2",
    "focus-visible": "^5.2.0",
    "formidable": "^1.2.6",
    "fs-extra": "^9.1.0",
    "karma": "^6.3.19",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-express-server": "^0.1.4",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "less": "^3.13.1",
    "less-loader": "^7.3.0",
    "less-plugin-est": "^3.0.1",
    "lint-staged": "^8.2.1",
    "mocha": "^7.2.0",
    "moment": "^2.29.3",
    "postcss": "^8.4.31",
    "promise-polyfill": "^8.2.3",
    "recursive-readdir": "^2.2.2",
    "regenerator-runtime": "^0.13.9",
    "rollup": "^2.72.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-vue": "^5.1.9",
    "sinon": "^13.0.2",
    "veui-loader": "workspace:*",
    "veui-theme-dls": "workspace:*",
    "veui-theme-dls-icons": "workspace:*",
    "vue": "^2.6.14",
    "vue-router": "^3.5.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "peerDependencies": {
    "@vue/runtime-dom": "^3.2.26",
    "vue": "^2.5.16"
  },
  "peerDependenciesMeta": {
    "@vue/runtime-dom": {
      "optional": true
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "keywords": [
    "vue",
    "ui",
    "components",
    "veui",
    "baidu"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "repository": "ecomfe/veui"
}
