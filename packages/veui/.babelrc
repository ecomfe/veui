{
  "presets": [
    ["env", { "modules": false }],
    "stage-2"
  ],
  "plugins": [
    [
      "veui",
      {
        "modules": [
          {
            "package": "veui-theme-x",
            "fileName": "${module}.less"
          },
          {
            "package": "veui-theme-x",
            "fileName": "${module}.js",
            "transform": false
          }
        ]
      }
    ],
    "lodash",
    "transform-vue-jsx",
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": false
      }
    ]
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
