{
  "name": "veui-theme-dls-icons",
  "version": "2.17.8",
  "description": "Icon pack for theme Light DLS of VEUI.",
  "repository": "ecomfe/veui",
  "license": "MIT",
  "keywords": [
    "veui",
    "vue",
    "baidu",
    "svg icons"
  ],
  "scripts": {
    "icons": "node -r esm build/icons.js",
    "build": "pnpm icons && rollup -c",
    "prepublishOnly": "pnpm build && cp -r ./icons/* .",
    "publish": "del '*.js' '!*.config.js' 'icon-names.json'",
    "prepare": "pnpm icons"
  },
  "dependencies": {
    "dls-icons-vue": "~3.36.1"
  },
  "peerDependencies": {
    "veui": "workspace:*"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "del-cli": "^4.0.1",
    "esm": "^3.2.25",
    "mkdirp": "^0.5.6",
    "rimraf": "^2.7.1",
    "rollup": "^2.72.0",
    "veui": "workspace:*",
    "vue": "^2.6.14"
  }
}
