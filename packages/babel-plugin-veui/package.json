{
  "name": "babel-plugin-veui",
  "version": "2.16.2",
  "description": "Import VEUI components with ease.",
  "main": "./index.js",
  "repository": "ecomfe/veui",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir .",
    "test": "export NODE_OPTIONS=--openssl-legacy-provider && pnpm build && ava",
    "prepare": "pnpm build",
    "publish": "rm -rf ./*.js"
  },
  "keywords": [
    "veui",
    "vue-components",
    "theme",
    "babel-plugin"
  ],
  "peerDependencies": {
    "veui": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "ava": "^3.15.0",
    "esm": "^3.2.25",
    "prettier": "^1.19.1",
    "veui": "workspace:*"
  },
  "ava": {
    "require": [
      "esm"
    ],
    "files": [
      "test/index.js"
    ]
  }
}
